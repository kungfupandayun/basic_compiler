class Taylor_series{
    int j=1;
  float val=1;
  float factorial=1;

  float power_fact(float rad, int indice)
  {  
    factorial=1;
    j=1;
    val=1;
    while(j<=indice)
    {
      val=val*rad;
      factorial=factorial*j;
      j=j+1;     
    }
    return (val/factorial);
  }



