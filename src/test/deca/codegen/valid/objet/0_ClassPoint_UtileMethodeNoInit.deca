class Point2D {
	int x; // Abscisse
	int y; // Ordonnee
	// Deplace ce point
	// de a en diagonale.
	void diag(int a) {
		x = x + a;
		y = y + a;
	}
}

{	
	Point2D p1;
	p1 = new Point2D();
	p1.diag(1);
	println("p1.x = ", p1.x);
}
